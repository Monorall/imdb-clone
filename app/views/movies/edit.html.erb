<h1>Editing <%= @movie.title %></h1>
<%= form_with(model: @movie) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :text %>
  <%= f.text_area :text %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.submit %>
<% end %>